{
    "$schema": "https://typedoc.org/schema.json",
    "entryPoints": [
        "./app"
    ],
    "entryPointStrategy": "expand", // Expand to include subdirectories
    "out": "docs",
    "exclude": [
        "**/*.test.ts",
        "**/*.spec.ts",
        "node_modules",
        "public/sw.js"
    ],
    "excludeExternals": true,
}